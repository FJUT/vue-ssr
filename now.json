{
    "version": 2,
    "name": "disqusjs-proxy",
    "alias": [
        "weixin.now.sh"
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "public, max-age=600",
                "access-control-allow-credentials": "true",
                "access-control-allow-origin": "*.kainy.cn"
            },
            "dest": "https://disqus.com/api/$1"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}
